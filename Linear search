#include<stdio.h>
int binary(int arr[],int key,int n);
{
	int first=0;
	int last=n-1;
	while(first<=last)
	{
		int mid=(first-last)/2;
		if(arr[mid]==key)
		 return mid;
		 else if(arr[mid]<key)
		 first=mid+1;
		 else
		 last=mid-1;
	}
	return 0;
}
int main()
{
	int arr[]={10,20,30,40,50};
	int n=sizeof(arr[])/size
}
